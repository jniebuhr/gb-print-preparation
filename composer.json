    "require": {
        "corneltek/cliframework": "2.4.*",
        "zendframework/zendpdf": "2.0.*"
    },
    "require-dev": {
        "phing/phing": "2.10.*",
        "phpmd/phpmd": "2.1.*",
        "phploc/phploc": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "phpunit/phpunit": "4.4.*",
        "sebastian/phpcpd": "2.0.*",
        "squizlabs/php_codesniffer": "2.2.*",
        "phpdocumentor/phpdocumentor": "2.8.*"
    },
    "autoload": {
        "psr-4": {
            "Goldbek\\Cards\\PrintPreparation\\": ["src/", "tests/"]
        }
